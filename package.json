{
  "name": "connecting-food-tech-test",
  "version": "1.0.0",
  "description": "Connecting Food technical test",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZiiQoOS/ConnectingFood-TechTest.git"
  },
  "author": "Ahmed-Zakaria ESDAIRI",
  "license": "MIT",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "concurrently -r \"npm:type-check:watch\" \"npm:start\"",
    "start": "parcel ./src/index.html",
    "build": "npm run type-check && parcel build ./src/index.html --public-url '/'",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "keywords": [
    "Typescript",
    "React",
    "Parcel"
  ],
  "dependencies": {
    "antd": "^4.1.0",
    "axios": "^0.19.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.9.4",
    "@types/node": "^11.13.6",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "concurrently": "^4.1.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.0",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.4"
  }
}
